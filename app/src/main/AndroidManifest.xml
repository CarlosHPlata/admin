<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usuario.androidadmin" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.view.Login"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:uiOptions="splitActionBarWhenNarrow" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.view.ListNotes"
            android:label="@string/title_activity_list_notes" >
            <intent-filter>
                <action android:name="com.view.ListNotes" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.view.OpenNote"
            android:label="@string/title_activity_open_note" >
            <intent-filter>
                <action android:name="com.view.OpenNote" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.model.service.ListNotesTest"
            android:label="@string/title_activity_list_notes_test" >
        </activity>
        <activity
            android:name="androidadmin.note.Index"
            android:label="@string/title_activity_index" >
        </activity>
        <activity
            android:name="androidadmin.note.New"
            android:label="@string/title_activity_new" >
        </activity>
        <activity
            android:name="androidadmin.note.Edit"
            android:label="@string/title_activity_edit" >
        </activity>
        <activity
            android:name="com.model.service.NoteManagerTest"
            android:label="@string/title_activity_note_manager_test" >
        </activity>
        <!--
        <activity
            android:name="com.view.Login"
            android:label="@string/title_activity_login" >
        </activity>
        -->

        <activity
            android:name="com.view.NewNote"
            android:label="@string/title_activity_new_note" >
        </activity>
    </application>

</manifest>
